        <link rel="stylesheet" href="<%= static_path(@conn, "/lib/durandal/css/durandal.css") %>" />
        <link rel="stylesheet" href="<%= static_path(@conn, "/css/starterkit.css") %>" />
        <link rel="stylesheet" href="<%= static_path(@conn, "/lib/toastr/toastr.min.css") %>" />
        
        <script type="text/javascript">
